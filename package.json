    "bcrypt": "^5.1.1",
    "chalk": "4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
